# QJoyPad 4.1 Layout File

Joystick 1 {
	Axis 1: +key %Right, -key %Left
	Axis 4: +key %Down, -key %Up
	Button 1: key %space
	Button 2: key %c
	Button 3: key %c
	Button 4: key %z
	Button 5: key %z
	Button 8: key %x
	Button 9: key %x
	Button 10: key %x
	Button 11: key %x
	Button 12: key %Escape
}

Joystick 2 {
	Axis 1: +key %Right, -key %Left
	Axis 4: +key %Down, -key %Up
	Button 1: key %Delete
	Button 2: key %c
	Button 3: key %c
	Button 4: key %z
	Button 5: key %z
	Button 8: key %x
	Button 9: key %x
	Button 10: key %x
	Button 11: key %x
	Button 12: key %Return
}

