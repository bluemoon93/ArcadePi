# QJoyPad 4.1 Layout File

Joystick 1 {
	Axis 1: +key %Right, -key %Left
	Axis 2: +key %Down, -key %Up
	Button 1: key %space
	Button 2: key %c # Afterburner
	Button 3: key %c # Afterburner
	Button 4: key %c # Afterburner
	Button 5: key %c # Afterburner
	Button 8: key %z # Guns
	Button 9: key %z # Guns
	Button 10: key %x # Missiles
	Button 11: key %x # Missiles
	Button 12: key %Escape
}

Joystick 2 {
	Axis 1: +key %Right, -key %Left
	Axis 2: +key %Down, -key %Up
	Button 1: key %Delete
	Button 2: key %c # Afterburner
	Button 3: key %c # Afterburner
	Button 4: key %c # Afterburner
	Button 5: key %c # Afterburner
	Button 8: key %z # Guns
	Button 9: key %z # Guns
	Button 10: key %x # Missiles
	Button 11: key %x # Missiles
	Button 12: key %Return
}

