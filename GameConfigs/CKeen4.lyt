# QJoyPad 4.1 Layout File

Joystick 1 {
	Axis 1: +key %Up, -key %Down
	Axis 4: +key %Left, -key %Right
	Button 1: key %space
	Button 2: key %Control_L
	Button 3: key %Control_L
	Button 4: key %Alt_L
	Button 5: key %Alt_L
	Button 8: key %space
	Button 9: key %space
	Button 10: key %space
	Button 11: key %space
	Button 12: key %Escape
}

Joystick 2 {
	Axis 1: +key %Up, -key %Down
	Axis 4: +key %Left, -key %Right
	Button 1: key %Delete
	Button 2: key %Control_L
	Button 3: key %Control_L
	Button 4: key %Alt_L
	Button 5: key %Alt_L
	Button 8: key %space
	Button 9: key %space
	Button 10: key %space
	Button 11: key %space
	Button 12: key %Return
}

# ARROWS space ctrl alt return esc