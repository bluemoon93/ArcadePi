# QJoyPad 4.1 Layout File

Joystick 1 {
	Axis 1: +key %Up, -key %Down
	Axis 4: +key %Left, -key %Right
	Button 1: key %Left
	Button 2: key %space
	Button 3: key %Left
	Button 4: key %Right
	Button 5: key %Up
	Button 8: key %Return
	Button 9: key %Left
	Button 10: key %Right
	Button 11: key %Down
	Button 12: key %Escape
}

Joystick 2 {
	Axis 1: +key %Up, -key %Down
	Axis 4: +key %Left, -key %Right
	Button 1: key %Delete
	Button 2: key %space
	Button 3: key %Left
	Button 4: key %Right
	Button 5: key %Up
	Button 8: key %Return
	Button 9: key %Left
	Button 10: key %Right
	Button 11: key %Down
	Button 12: key %Right
}

# left right space return up down